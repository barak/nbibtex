#!/usr/bin/make -f
# -*- makefile -*-
# copy of /usr/share/doc/debhelper/examples/rules.simple

build: build-stamp
build-stamp:
	dh build
	touch build-stamp

clean:
	dh clean

install: build install-stamp
install-stamp:
	dh install
	touch install-stamp

binary-arch: install
	dh binary-arch

binary-indep: install
	dh binary-indep

binary: binary-arch binary-indep

.PHONY: build clean install binary-arch binary-indep binary
